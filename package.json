{
  "name": "@oclif/plugin-test-core-v3",
  "description": "Test plugin that uses @oclif/core v3",
  "version": "0.1.62",
  "author": "Salesforce",
  "bin": {
    "corev3": "./bin/run.js"
  },
  "bugs": "https://github.com/oclif/plugin-test-core-v3/issues",
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9.29.0",
    "eslint-config-oclif": "^6.0.72",
    "eslint-config-prettier": "^10.1.5",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/oclif/plugin-test-core-v3",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "corev3",
    "dirname": "corev3",
    "commands": "./dist/commands",
    "hooks": {
      "init": "./dist/hooks/init/init.js"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "oclif/plugin-test-core-v3",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
